# Container configuration
CONTAINER_NAME=bedrock-server
SERVER_NAME=MyBedrockServer
LEVEL_NAME=MyWorld
SEED=123456789
GAMEMODE=survival
DIFFICULTY=normal
MAX_PLAYERS=10
SERVER_PORT=19132

# Remote access
ENABLE_TUNNEL=true
TUNNEL_TYPE=ngrok
TUNNEL_AUTHTOKEN=your-ngrok-token

# Backup configuration
BACKUP_DIR=./backups
MAX_BACKUPS=5
REMOVAL_PASSWORD=secret123

# Environment hash storage
ENV_HASH_FILE=.env.hash
